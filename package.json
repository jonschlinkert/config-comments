{
  "name": "commandments",
  "description": "Parse argument commands/options from code comments using esprima and minimist.",
  "version": "0.2.3",
  "homepage": "https://github.com/jonschlinkert/commandments",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/commandments",
  "bugs": {
    "url": "https://github.com/jonschlinkert/commandments/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "extract-comments": "^0.8.6",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*"
  },
  "keywords": [
    "args",
    "argument",
    "arguments",
    "argv",
    "code",
    "command",
    "commands",
    "comment",
    "comments",
    "esprima",
    "getopt",
    "minimist",
    "opt",
    "optimist",
    "option",
    "options",
    "parse",
    "parser"
  ],
  "verb": {
    "related": {
      "list": [
        "lint-deps"
      ]
    },
    "plugins": [
      "gulp-format-md"
    ],
    "layout": "default"
  }
}
